{
  "name": "distro",
  "version": "0.0.1",
  "description": "porcelain front-end of gnu/linux distros package managers",
  "main": "./bin/distro",
  "bin": "./bin/distro.ts",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc --reporter=text mocha",
    "lint": "eslint --ext .js,.yml -f ./node_modules/eslint-friendly-formatter . --fix",
    "start": "bin/distro.js",
    "audit": "npm audit",
    "fmt": "npx prettier --write src/*",
    "index": "node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~easbarba/distro.js"
  },
  "keywords": ["apt", "apk", "dnf", "pacman"],
  "author": "Eas Barbosa",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jsdoc": "^3.6.7",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "prettier": "2.5.1"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "chalk": "^5.0.0",
    "commander": "^8.3.0",
    "js-yaml": "^4.1.0",
    "pino": "^7.6.3",
    "pino-pretty": "^7.3.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0"
  }
}
